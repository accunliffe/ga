@import url('http://fonts.googleapis.com/css?family=Raleway')
@font-face 
  font-family:'leagueG'
  src url('/fonts/LeagueGothic-Regular.otf')
@font-face 
  font-family:'Code'
  src url('/fonts/Code.otf')
@font-face 
  font-family:'MSans'
  src url('/fonts/M-Sans.ttf')
@font-face 
  font-family:'Arca'
  src url('/fonts/Arca-Heavy.ttf')
  

body
  overflow none
  background-color #212121
  background-size cover
  background-position left top
  
.row-fluid
  text-align center
  
.bodyWrapper
  height 100%
  width 100%  

.container-fluid.hidden-phone.hidden-tablet
  margin 0
  padding 0
  height auto
  text-align center

#nav.navbar
  background-color rgba(110,110,110,0.40)
  position fixed
  z-index 2
  min-width 100%
  text-align center
  border-bottom 1px solid white
  height 80px
  
img.logo3
  float left
  display inline-block
  padding-top 6px
  padding-left 15px
  padding-bottom 7px
  height 40px
  min-width 60px
  cursor pointer
  
img.logo5.desktop
  @media screen and (min-width: 1830px)
    padding-top 8px
  @media screen and (min-width: 2300px)
    padding-top 1.5px
  float right
  position relative
  display inline-block 
  padding 10px 5px 10px 0
  min-width 60px
  max-height 70px
  cursor pointer
  -webkit-font-smoothing antialiased

h3.navtext1
  @media screen and (min-width: 1700px)
    padding-top 10px
    margin-top .98%
  @media screen and (min-width: 2300px)
    margin-top 1.1%
    padding-top 3px
  display inline-block
  font-family:'Arca'
  color white
  padding-top 10px
  padding-bottom 0
  padding-right 45px
  letter-spacing 3px
  text-align center
  font-size 120%

h3.navtext
  display inline-block
  padding-left 10px
  font-family:'MSans'
  color white
  margin-top 15px
  -webkit-font-smoothing antialiased
  letter-spacing 3px


.menubutton.desktop
  text-align left
  display inline-block
  float right
  padding-top 0
  padding-bottom 20px
  
h3.menutext
  text-align left
  float left
  display inline-block
  color white
  -webkit-font-smoothing antialiased
  letter-spacing 3px
  font-size 16px
  padding 10px 0 0 10px

ul
  &.dropdown-menu
    background-color rgba(110,110,110,0.55)
    margin-right 1px
        


.dropdown-menu
  & li
    & a
      -webkit-font-smoothing antialiased      
      font-family:'MSans'
      color white
      font-size 120%
      
.dropdown-menu
  & li.contact
    & a
      -webkit-font-smoothing antialiased      
      font-family:'MSans'
      color #D0DCF7
      font-size 120%
      
section
  &.module   
    &:last-child
      margin-bottom 0      
    h2
      margin-bottom 30px
      font-size 60px
      padding-bottom 0
      padding-top 20px
    h3
      font-size 40px
      padding-bottom 30px
    p
      display inline-block
      font-family:'Raleway'
      margin-bottom 40px      
      font-weight 300
      font-size 18px
      height auto
      padding-bottom 40px
      line-height 30px
      text-align center
      &:last-child
        margin-bottom 0
      &.verticalLine
        width 1%
        height 600px
        border-right medium solid #D3D3D3
      &.left
        width 49.5%
        height auto
        float left
        padding-left 5px
      &.right
        width 49.5%
        height auto
        float right
        text-align left !important
        padding-left 20px
        padding-right 10px
      
    &.content
      padding 0 0
      height 950px
      background-color white
    &.contentM
      padding 0 0
      height 400px
      background-color white
    &.parallax
      @media screen and (max-width: 1150px)
        height 550px
      @media screen and (min-width: 1700px)
        height 1200px
      padding-top 5%
      height 600px
      background-position center center
      background-repeat no-repeat
      background-attachment fixed
      background-size cover
      h1
        color rgba(255, 255, 255, 0.8)
        font-size 48px
        line-height 800px
        font-weight 700
        text-align center
        text-transform uppercase
        text-shadow 0 0 10px rgba(0, 0, 0, 0.2)
        font-size 96px
      h3
        color blue
        padding-top 25%
        padding-bottom 0
      label
        color blue
      p
        padding 0
        margin 0 0 15px 0
        color blue
      &.parallax-1
        background-image url("/images/nursery2.jpg")
        
form.hero
  width 30%
  text-align center
  position relative
  display inline-block

#btn
  text-align center !important
  position relative
  display inline-block
  background-color rgba(110,110,110,0.55)    
  width auto
  height 50px
  border-color #ffffff
.moder
  margin 2.5% 32.5px 0 32.5px !important
  width auto
  height auto
  display inline-block

.icon-wrapper
  color #ffffff

#btn2
  position relative
  background-color rgba(214,214,214,0.60)    
  width auto
  height auto
  border-color #ffffff
  
//modal styles
.modal-dialog
  width 95%
  height 90%

.modal-header
  height 60px
  text-align center
  width auto

.modal-body
  height auto
  text-align center

.buttonWrapper
  height 100px
  
h3.modal-title
  width auto
  display inline-block
  
button.btn.btn-warning
  float right
  background-color rgba(214,214,214,0.60)  
  border-color #ffffff
  color #444345
  display inline-block
 
  
button.btn.btn-warning.prot
  float none
  background-color rgba(84,115,240,0.90)
  color white
  position relative
  margin-top 30px
  height 80px
  width 10%
  font-size 120%
  min-width 200px
  display inline-block
  margin 10px 10px 10px 10px
  
button.btn.btn-warning.resume
  float none
  background-color rgba(240,173,78,0.90)
  color white
  position relative
  margin-top 30px
  height 80px
  width 10%
  font-size 120%
  min-width 200px
  display inline-block
  margin 10px 10px 10px 10px
  
button.btn.btn-warning.bottom
  background-color rgba(214,214,214,0.90)  
  border-color #ffffff
  color #444345
  float none
  position relative
  margin-top 30px
  height 80px
  width 10%
  font-size 120%
  min-width 200px
  display inline-block
  margin 10px 10px 10px 10px
  
.verticalLine
  width 1%
  height 600px
  border-right medium solid #D3D3D3
  display inline-block
  position relative
  
.left
  float left
  display inline-block
  position relative
  width 48.5%

.right
  float right !important
  display inline-block
  position relative
  width 48.5%
  text-align center
  
h4.modalContent.topper
  max-width 1000px
  border-bottom 1px solid #D3D3D3
  text-align left
  margin 0 auto 10px auto
  padding-top 40px
  
h4.modalContent
  max-width 1000px
  border-bottom 1px solid #D3D3D3
  text-align left
  margin 0 auto 10px auto
  padding-top 40px
  
p.moder1
  text-align left
  margin 15px auto 25px auto
  max-width 1000px
  font-size 120%
  
p.moder2
  text-align center
  margin 30px auto 30px auto
  max-width 1000px
  font-size 120%
  
.zoom
  width 80%
  height 80%
  -webkit-transition 1.4s ease-in-out
  -moz-transition 1.4s ease-in-out
  -o-transition 1.4s ease-in-out
  transition 1.4s ease-in-out
  &:hover
    width 100% !important
    height 100% !important